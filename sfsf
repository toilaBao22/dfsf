game.StarterGui:SetCore("SendNotification", {
    Title = "Script Bao X Viet Hub";
    Text = "Make  by Bao and Viet ";
    Duration = 10;
})

game.StarterGui:SetCore("SendNotification", {
    Title = "How to open menu??";
    Text = " Alt Right  to open menu "; 
    Duration = 10;
})

local UserInputService = game:GetService("UserInputService")

