UserInputService.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.RightAlt then
		menuchinh.Visible = not menuchinh.Visible

		if menuchinh.Visible then
            		hienanmenu.Text = "ON"
			hienanmenu.TextColor3 = Color3.fromRGB(62, 255, 32)
		else
        			hienanmenu.Text = "OFF"
			hienanmenu.TextColor3 = Color3.fromRGB(255, 0, 0)
		end
	end
end)

local script = Instance.new('Script', gamedeadrails)
local gamedeadrails = script.Parent
local khungmenudeadrails = gamedeadrails.Parent.Parent.khungmenudeadrails
gamedeadrails.MouseButton1Click:Connect(function()
	khungmenudeadrails.Visible = true
end)

local script = Instance.new('Script', teletoend)
local teletoend = script.Parent
teletoend.MouseButton1Click:Connect(function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/2dfeta/LamViet/refs/heads/main/teleport_to_end.lua"))()
end)

